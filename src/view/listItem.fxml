<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<fx:root stylesheets="@darkstyle.css" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="listItemTitle" />
    <AnchorPane prefWidth="300" />
    <Button fx:id="deleteButton" layoutX="300" onAction="#deleteButtonClicked" text="Delete" />
    <Button fx:id="viewButton" onAction="#viewButtonClicked" text="View" />
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</fx:root>
